{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "deprecated": false,
  "description": "Wrap words to a specified length.",
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "homepage": "https://github.com/jonschlinkert/word-wrap",
  "license": "MIT",
  "main": "index.js",
  "name": "word-wrap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/word-wrap.git"
  },
  "typings": "index.d.ts",
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "common-words",
        "shuffle-words",
        "unique-words",
        "wordcount"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  },
  "version": "1.2.3"
}