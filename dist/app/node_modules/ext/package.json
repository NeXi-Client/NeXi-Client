{
  "author": {
    "name": "Mariusz Nowak",
    "email": "medyk@medikoo.com",
    "url": "http://www.medikoo.com/"
  },
  "dependencies": {
    "type": "^2.0.0"
  },
  "deprecated": false,
  "description": "JavaScript utilities with respect to emerging standard",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.7.1",
    "eslint-config-medikoo": "^2.7.0",
    "git-list-updated": "^1.2.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "prettier-elastic": "^1.18.2",
    "sinon": "^7.5.0"
  },
  "eslintIgnore": [
    "_es5-ext/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{css,html,js,json,md,yaml,yml}": [
      "prettier -c"
    ]
  },
  "mocha": {
    "recursive": true
  },
  "name": "ext",
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4,
    "quoteProps": "preserve",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medikoo/es5-ext/tree/ext"
  },
  "version": "1.4.0"
}