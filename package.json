{
  "name": "nexi-client",
  "version": "1.2.1",
  "description": "NeXi-Client is a client made for the browser game called 'Venge.io'",
  "main": "main.js",
  "scripts": {
    "start": "electron . ",
    "postinstall": "install-app-deps",
    "build-win": "electon-builder -w",
    "build-mac": "electon-builder -m",
    "build-linux": "electon-builder -l",
    "build-all": "electon-builder -mwl"
  },
  "build": {
    "appId": "org.electron.NeXiClient"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeXiDE/NeXi-Client.git"
  },
  "author": "NeXi",
  "license": "ISC",
  "dependencies": {
    "dialogs": "^2.0.1",
    "discord-rpc": "^3.1.3",
    "discord.js": "^12.3.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-packager": "^15.1.0",
    "electron-prompt": "^1.6.0",
    "electron-updater": "^4.3.4",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "electron": "^10.0.1",
    "electron-builder": "^22.8.0"
  }
}